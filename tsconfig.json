{
    "compilerOptions":
    {
        "outDir": "./dist/",
        "noImplicitAny": false,
        "sourceMap": true,
        "module": "ESNext",
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "experimentalDecorators": true,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "target": "es5",
        "typeRoots":
        [
            "node_modules/@types",
            "typings"
        ]
    },
    "include":
    [
        "./**/*.ts"
    ]
}