{
    "compilerOptions":
    {
        "outDir": "./dist/",
        "sourceMap": true,
        "module": "es6",
        "moduleResolution": "node",
        "noImplicitAny": false,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "target": "es5",
    },
    "include": [ "./src/**/*.ts" ],
    "exclude": [ "node_modules", "**/*.spec.ts" ]
}
